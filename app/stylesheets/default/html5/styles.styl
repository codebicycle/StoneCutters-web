* {
	margin: 0; 
	padding: 0;
}
body {
	font-family: Helvetica,Arial,sans-serif;
}
a {
	text-decoration: none;
	outline: none;
}
.env {
	padding: .2rem; 
	background: #333; 
	font-size: 10px; 
	color: #FFF;
}
#title {
	display: block;
	padding: 0.3rem 1rem;
	background: #a0a0a0;
	position: relative;
}
#title a {
	width: 11px;
	height: 18px;
	float: left;
	margin: .25rem .5rem;
	background: url('/images/html5/back.png');
}
#title h1 {
	color: #fff;
	font-size: 1.25rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
