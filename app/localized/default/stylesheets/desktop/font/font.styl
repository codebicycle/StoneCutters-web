@font-face {
    font-family: 'olx-icon-font';
    src: url(staticUrl/fonts/olx-icon-font.eot);
    src: url(staticUrl/fonts/olx-icon-font.eot?#iefix) format('embedded-opentype'),
         url(staticUrl/fonts/olx-icon-font.woff) format('woff'),
         url(staticUrl/fonts/olx-icon-font.ttf) format('truetype'),
         url(staticUrl/fonts/olx-icon-font.svg#olx-icon-fontregular) format('svg');
    font-style: normal;
    font-weight: normal;
}
@font-face {
  font-family: 'kalam';
    src: url(staticUrl/fonts/kalam-bold.eot);
    src: url(staticUrl/fonts/kalam-bold.eot?#iefix) format('embedded-opentype'),
         url(staticUrl/fonts/kalam-bold.woff) format('woff'),
         url(staticUrl/fonts/kalam-bold.ttf) format('truetype'),
         url(staticUrl/fonts/kalam-bold.svg#kalam_boldregular) format('svg');
    font-weight: normal;
    font-style: normal;
}
.icons::before {
    display: inline-block;
    font-family: 'olx-icon-font';
    font-style: normal;
    font-weight: normal;
    line-height: 1;
    speak: none;
    vertical-align: middle;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

/* Por favor, cada vez que se agrege un icono nuevo, ordenarlo alfabéticamente para su rápida ubicación */
