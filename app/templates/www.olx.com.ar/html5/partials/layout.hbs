<!doctype html>
<html>
	<head>
		{{#layout template '/partials/head'}}
			{{{partial layout}}}
		{{/layout}}
	</head>
	<body>
		
		{{view "header/index"}}
		<article>
	  		{{view "left-panel/index"}}
			<div id="content">
				{{{body}}}
				{{view "footer/index"}}
			</div>
		</article>

		<script src="/js/libs.js"></script>
		<script src="/js/app.js"></script>
		<script>
			(function() {
				var App = window.App = new (require('app/app'))({{json appData}});
				App.bootstrapData({{json bootstrappedData}});
				App.start();
			})();
		</script>
	</body>
</html>
