@font-face {
    font-family: 'olx-icon-font';
    src: url(staticUrl/fonts/olx-icon-font-9.eot);
    src: url(staticUrl/fonts/olx-icon-font-9.eot?#iefix) format('embedded-opentype'),
         url(staticUrl/fonts/olx-icon-font-9.woff) format('woff'),
         url(staticUrl/fonts/olx-icon-font-9.ttf) format('truetype'),
         url(staticUrl/fonts/olx-icon-font-9.svg#olx-icon-font-9regular) format('svg');
    font-style: normal;
    font-weight: normal;
}
@font-face {
  font-family: 'kalam';
    src: url(staticUrl/fonts/kalam-bold.eot);
    src: url(staticUrl/fonts/kalam-bold.eot?#iefix) format('embedded-opentype'),
         url(staticUrl/fonts/kalam-bold.woff) format('woff'),
         url(staticUrl/fonts/kalam-bold.ttf) format('truetype'),
         url(staticUrl/fonts/kalam-bold.svg#kalam_boldregular) format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'HelveticaRoundedLTStd-Bd';
    src: url(staticUrl/fonts/HelveticaRoundedLTStd-Bd.eot?#iefix) format('embedded-opentype'),
         url(staticUrl/fonts/HelveticaRoundedLTStd-Bd.otf) format('opentype'),
         url(staticUrl/fonts/HelveticaRoundedLTStd-Bd.woff) format('woff'),
         url(staticUrl/fonts/HelveticaRoundedLTStd-Bd.ttf) format('truetype'),
         url(staticUrl/fonts/HelveticaRoundedLTStd-Bd.svg#HelveticaRoundedLTStd-Bd) format('svg');
    font-weight: normal;
    font-style: normal;
}
.icons::before {
    display: inline-block;
    font-family: 'olx-icon-font';
    font-style: normal;
    font-weight: normal;
    line-height: 1;
    speak: none;
    vertical-align: middle;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

