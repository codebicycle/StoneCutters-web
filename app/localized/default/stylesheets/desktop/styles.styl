@import 'helpers/normalize.css'
@import 'font/font.css'

body {
	background: #fafafa;
	font-family: arial, helvetica, sans-serif;
	font-size: 16px;
}

header {
	position: relative;
}

.clearfix::after {
	content: " ";
	clear: both;
	display: block;
}

.content-centered {
	margin: 10px auto;
	max-width: 980px;
}

.enviroment {
	background: #ff0;
	color: #fff;
	left: 0;
	padding: 10px;
	position: absolute;
	top: 0;
}