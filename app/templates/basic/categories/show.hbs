{{category.name}}
<table width="100%" cellspacing="0" cellpadding="4" border="0" align="left">
	{{#each category.children}}
		<tr>
			<td style="border-top: solid thin #808080;">
				<a style="display: block;" href="/items?categoryId={{id}}&location={{../params.location}}">
					<small style="display: block;"><img border="0" alt="" src="" />{{trName}}</small>
				</a>
			</td>
		</tr>
	{{/each}}
</table>
