#listing-filters {
    width: 238px;
    float: left;
    margin-right: 20px;
    box(#eee, #ddd, 3px, 0);
    > span {
        display: block;
        margin: 0;
        padding: 10px;
        font-weight: bold;
        font-size: 1.077rem;
    }
    span.title {
        position: relative;
        border-bottom: 1px solid #ddd;
        color: #000;
        border: none;
        &.filter-title{
            font-size: 1rem;
        }
        .icons {
            position: absolute;
            right: 5px;
            top: 5px;
            font-size: 1.5rem;
            color: #999;
        }
    }
    a {
        display: block;
        padding: 10px;
        &:before {
            font-size: 1.8rem;
        }
    }
}
#listing-items{
    width: 720px;
    float: left;
}
