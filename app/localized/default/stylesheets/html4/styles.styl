* {
	margin: 0;
	padding: 0;
}
body {
	font-family: Helvetica,Arial,sans-serif;
}
a {
	text-decoration: none;
	outline: none;
}
ul, li {
	list-style: none;
}
ul.normalList {
	padding: 0 16px;

}
ul.normalList li {
	position: relative;
	border-bottom: 1px solid #CCCCCC;
}
ul.normalList li h2 {
	font-size: 16px;
}
ul.normalList li a {
	display: block;
	padding: 8px 0;
	color: #333333;
	line-height: 24px;
	text-transform: capitalize;
}
.clear {
	clear: both;
}
.env {
	padding: .2rem;
	background: #333;
	font-size: 10px;
	line-height: 14px;
	color: #FFF;
    text-align: center;
}
.active {
    font-weight: bold;
}
.last {
	border: none !important;
}
.line {
	height: 1px;
	display: block;
	margin: 6px 0;
	background: #cccccc;
}
.breadcrumb {
	height: 32px;
	position: relative;
	overflow: hidden;
	background: #a0a0a0;
}
.breadcrumb a.arrow {
	width: 38px;
	height: 32px;
	float: left;
	border-right: 1px solid #676767;
	background: url(imageUrl/images/html4/navBack.png) no-repeat center;
	text-indent: -9000px;
}
.breadcrumb a.arrow.home {
	border-right: none;
	border-left: 1px solid #c9c9c9;
	background: url(imageUrl/images/html4/navHome.png) no-repeat center;
}
.breadcrumb h1,
.breadcrumb h3,
.breadcrumb strong {
	display: block;
	height: 25px;
	padding: 7px 16px 0;
	border-left: 1px solid #c9c9c9;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	font-size: 16px;
	color: #ffffff;
}
.breadcrumb a.logout {
	position: absolute;
	right: 10px;
	top: 8px;
	font-size: 11px;
	color: #ffffff;
}
.btns {
	width: auto;
	padding: 5px 7px;
	border-radius: 5px;
	color: #fff;
	font-weight: bold;
	cursor: pointer;
}
.btns.orange {
	border: 1px solid #C44D13;
	background: #E0771B;
}
.btns.grey {
	border: 1px solid #4c4b4b;
	background: #666666;
}
.btns.blue {
	border: 1px solid #2C4B77;
	background: #355E97;
}
.btns.green {
	border: 1px solid #8c9603;
	background: #a8b301;
}
.emtyList {
	padding: 28px 14px;
	font-size: 14px;
	color: #333333;
	text-align: center;
}
.emtyList div {
    width: 150px;
    height: 146px;
    display: inline-block;
    background: url(imageUrl/images/common/noResult.png);
}
.emtyList p {
    margin: 10px 0;
    font-size: 16px;
    color: #333;
    display: inline-block;
}
.emtyList a {
    display: inline-block;
    font-weight: bold;
}
/*----Titles----*/
h1, h2, h3, h4 {
	display: block;
	overflow: hidden;
	font-weight: normal;
}
h1 {
	font-size: 20px;
	color: #494949;
}
h2 {
	font-size: 20px;
	color: #E2771C;
}
h3 {
    color: #333333;
    font-size: 16px;
}
h3 a {
    font-size: 12px;
    color: #4275BA;
    text-decoration: none;
}
h4 {
	padding: 8px 16px 0;
	font-size: 16px;
	color: #333333;
	font-weight: bold;
}
/*---Forms****/
.formField {
	padding: 8px 16px;
	clear: both;
	overflow: hidden;
}
.formActions {
	padding: 16px;
	clear: both;
	text-align: left;
}
.formActions .btns {
	padding: 7px 10px;
	font-size: 12.8px;
}
.formActions .btns.w100 {
	width: 100%;
}
.formField label {
	display: block;
	padding: 0 0 4px 0;
	color: #333333;
}
.formField label > a {
	color: #4275BA;
	text-decoration: underline;
}
.formField > a {
	text-decoration: underline;
}
.formField > a {
	font-size: 11px;
	color: #0000EE;
}
.formField > label > small {
	display: block;
	padding: 2.5px 0 0;
	font-size: 10px;
	line-height: 13px;
	color: #ff6700;
}
.formField > small {
	display: block;
	padding: 2.5px 0 0;
	font-size: 10px;
	line-height: 13px;
	color: #333333;
}
.formField > input, .formField > textarea {
	width: 99%;
	display: block;
	padding: 4px 0 4px 4px;
	border-radius: 4px;
	border: 1px solid #cccccc;
	font-size: 16px;
	background: #f8f8f8;
}
.formField > input {
	height: 22px;
}
.formField > input.checkbox {
	width: auto;
	float: left;
	margin: 0 7px 0 0;
}
.formField > textarea {
	height: 72px;
}
.messages {
	margin: 0;
    padding: 6px 12px;
    clear: both;
    font-size: 0.8em;
    overflow: hidden;
    color: #333333;
}
.errors {
    border: 1px solid #B55351;
    background: #F9E4E3;
}
.feedback {
    border: 1px solid #9ba145;
    background: #dbe295;
}
.tracking {
    display: none;
}
/*----Itemlisting----*/
ul.itemListing li {
	width: 100%;
	height: 90px;
	border-bottom: 1px solid #cccccc;
}
ul.itemListing li a {
	display: block;
	color: #333333;
}
ul.itemListing li a .imageCont {
	width: 35%;
	max-width: 150px;
	height: 90px;
	float: left;
	margin: 0 8px 0 0;
	overflow: hidden;
	border-right: 1px solid #ececec;
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
}
ul.itemListing li .itemInfo {
	height: 90px;
	position: relative;
	overflow: hidden;
	color: #323232;
}
ul.itemListing li .itemInfo span {
	float: left;
	clear: left;
	margin: 2px 0;
	font-size: 11px;
	color: #777777;
}
ul.itemListing li .itemInfo .centerCont {
	width: 98%;
	height: 33px;
	float: left;
	clear: both;
	position: relative;
	overflow: hidden;
	margin: 3px 0;
}
ul.itemListing li .itemInfo .centerCont a h2 {
	height: 33px;
	display: table-cell;
	font-size: 14px;
	line-height: 17px;
	overflow: hidden;
	vertical-align: middle;
	color: #333333;
}
ul.itemListing li .itemInfo .itemPrice {
	float: right;
	font-size: 16px;
	color: #fd7600;
	margin: 5px 8px 0 0;
}

/*---- shops ----*/
.mapShops {
	margin-bottom: 5px;
	.nearby {
		border-bottom: 1px solid #8900d0;
		color: #8900d0;
		text-align: center;
		font-size: 12px;
		padding: 5px 0;
	}
}
.itemListing {
	.adLikeStore,
	.callToAction {
		background: #f7f7f7;
		.imageCont {
			background: #fff;
		}
	}
	.adLikeStore {
		a {
			position: relative;
		}
		.itemInfo {
			padding-right: 10px;
		}
		.flag {
			position: absolute;
			width: 35%;
			max-width: 150px;
			height: 21px;
		}
		.centerCont {
			height: auto !important;
			padding: 5px 0 10px 0;
			h2 {
				font-size: 14px;
				color: #333;
			}
		}
	}
	.callToAction {
		height: 120px;
		.imageCont {
			border-bottom: 1px solid #ececec;
		}
		.buttons {
			float: left;
			width: 100%;
			position: relative;
			.name {
				color: #777;
				float: left;
				font-size: 11px;
				font-style: italic;
				background: url(imageUrl/images/html4/stores/storeIcon.png) 0 50% no-repeat;
				padding: 1px 0 1px 20px;
				margin-left: 5px;
			}
			ul {
				position: absolute;
				right: 5px;
				top: -5px;
				li {
					width: auto;
					height: auto;
					display: inline-block;
					border: 0;
				}
				a {
					text-indent: -9999px;
					width: 25px;
					height: 21px;
					border: 1px solid #ddd;
					border-radius: 2px;
					&.mail {
						background: url(imageUrl/images/html4/stores/mail.png) 50% 0 no-repeat #fff;
					}
					&.phone {
						background: url(imageUrl/images/html4/stores/phone.png) 50% 2px no-repeat #fff;
					}
					&.sms {
						background: url(imageUrl/images/html4/stores/sms.png) 50% 3px no-repeat #fff;
					}
				}
				&.green-icons {
					a {
						&.mail {
							background: url(imageUrl/images/html4/stores/mail_green.png) 50% 0 no-repeat #fff;
						}
						&.phone {
							background: url(imageUrl/images/html4/stores/phone_green.png) 50% 2px no-repeat #fff;
						}
						&.sms {
							background: url(imageUrl/images/html4/stores/sms_green.png) 50% 3px no-repeat #fff;
						}
					}
				}
			}
		}
	}
}

.errorPage {
	&.coming-soon {
		img {
			width: 140px;
			display: block;
			margin: 30px auto 10px;
		}
		p.center {
			text-align: center;
			font-size: 22px;
		}
		p {
			margin-bottom: 10px;
		}
	}
}