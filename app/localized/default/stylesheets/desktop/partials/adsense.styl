#slot_empty {
    border: 1px solid red;
}
.adsense-listing iframe {
    box-sizing: border-box;
    border: 1px solid $greyBorders;
    border-bottom: none;
    display: block;
    margin-top: 10px;
}

.adsense-listing + .items-list {
    margin-top: 0;
}

.items-list + .adsense-listing {
    margin-bottom: 10px;
    margin-top: -10px;
}

.items-list + .adsense-listing iframe {
    border: 1px solid $greyBorders;
    border-top: none;
}

