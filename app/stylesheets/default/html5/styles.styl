* {
	margin: 0; 
	padding: 0;
}
body {
	font-family: Helvetica,Arial,sans-serif;
}
a {
	text-decoration: none;
	outline: none;
}
ul, li {
	list-style: none;
}
.noscroll{
	overflow: hidden;
	position: fixed;
}
.visible {
	display: block;
}
.clear {
	clear: both;
}
.env {
	padding: .2rem; 
	background: #333; 
	font-size: 10px; 
	color: #FFF;
}
#title {
	height: 1.625rem;
	display: block;
	padding: 0.3rem .5rem;
	background: #a0a0a0;
	position: relative;
}
#title a {
	width: 11px;
	height: 18px;
	float: left;
	margin: .25rem .5rem;
	background: url(imageUrl/images/html5/back.png);
}
#title h1 {
	float: left;
	color: #fff;
	font-size: 1.25rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.btns {
	width: auto;
	border-radius: 0.5rem;
	color: #fff;
}
.btns.orange {
	border: 1px solid #BD5901;
	background: #FF7700;
}
.btns.grey {
	border: 1px solid #4c4b4b;
	background: #666666;
}
.btns.blue {
	border: 1px solid #2C4B77;
	background-color: #355E97;
}
.btnCenter {
	display: block;
	text-align: center;
}
.emtyList {
	padding: 1.75rem .875rem;
	font-size: 0.875rem;
	font-weight: bold;
	color: #333333;
}



