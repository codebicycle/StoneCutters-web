footer#languages {
    padding: 0.5rem 0;
    text-align: center;
    background-color: #ddd;
}
footer#languages ul li {
    display: inline;
    padding: 0 .5rem;
    border-left: 1px solid #B5B5B5;
    font-size: 0.8125rem;
}
footer#languages ul li:first-child {
    border: none;
}
footer#languages ul li a, footer#languages ul li strong {
    color: #333;
}