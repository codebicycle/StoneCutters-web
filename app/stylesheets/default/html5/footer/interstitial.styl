#interstitial{background-image:url(imageUrl/images/common/marketing/backgroundInt200.jpg);background-repeat:repeat-y;background-size:100%;text-align:center;font-size:16px}
@media screen and (min-width:200px) and (max-width:299px){#interstitial{background-image:url(imageUrl/images/common/marketing/backgroundInt300.jpg)}}
@media screen and (min-width:300px){#interstitial{background-image:url(imageUrl/images/common/marketing/backgroundInt400.jpg)}}
#interstitial .header{width:100%;display:block;margin:10px auto}
#interstitial #logoTop{width:20%;margin:0;display:inline;vertical-align:top}
#interstitial h1{width:60%;display:inline;margin:0 0 0 .3em;0;display:inline;text-align:left;font-size:1.5em;display:inline;font-weight:normal;line-height:3.5em;text-transform:capitalize}
#interstitial #mobileImg{width:70%;margin:0 auto}
#interstitial .btn-adjust{width:80%;display:block;margin:10px auto}
#interstitial .btn-adjust a{width:80%;min-height:25px;padding-top:10px;background:#c7c727;border-color:#8f8f15;font-size:1em}
#interstitial .btn-adjust.btnGrey a{height:10px;padding-top:10px}
#interstitial .continue{width:100%;text-align:center;margin:15px 0 0 0}
#interstitial .continue a{color:#515151;text-decoration:none}