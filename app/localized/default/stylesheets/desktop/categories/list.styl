section#homeIcons {
	width: 41.25rem;
	float: left;
	border: 1px solid #ccc;
    border-radius: 3px;

    .titleBar {
    	width: 100%;
    	padding: .9375rem 1.25rem;
    	background: #f1f1f1;
    	border-bottom: 1px solid #ccc;

    	h1 {
			display: inline;
			font-size: 1.5rem;
			line-height: 1.875rem;
			margin: 0 .625rem 0 0 ;
			font-weight: normal;
			color: #000;
    	}
    	span {
    		display: inline-block;
    		font-size: .875rem;
    		color: #0076be;
    		cursor: pointer;
    		&:after {
    			display: inline-block;
				margin: -1px 0 1px 5px;
				border-color: #777 transparent;
				border-style: solid;
				border-width: .4375rem .375rem 0;
				content: "";
			}
    	}
    }

    > ul {
    	display: inline-block;
    	margin: .625rem 0 0 1.25rem;

    	> li {
    		width: 12.81rem;
    		height: 9.813rem;
    		float: left;
    		padding: .625rem .3125rem;
    		text-align: center;
    		&:hover {
    			background: #f1f1f1;
    			border-radius: 3px;
    		}

    		span.icon {
    			width: 100%;
                height: 64px;
    			float: left;
    			margin: 0;
    			background: url(imageUrl/images/desktop/spriteIconsVE.png);
    			background-repeat: no-repeat;

    			&.cat-830 {
    				background-position: center 0;
    			}
    			&.cat-800 {
    				background-position: center -64px;
    			}
    			&.cat-806 {
    				background-position: center -128px;
    			}
    			&.cat-815 {
    				background-position: center -192px;
    			}
    			&.cat-362 {
    				background-position: center -256px;
    			}
    			&.cat-811 {
    				background-position: center -320px;
    			}
    			&.cat-859 {
    				background-position: center -384px;
    			}
    			&.cat-190 {
    				background-position: center -448px;
    			}
    			&.cat-191 {
    				background-position: center -512px;
    			}
    			&.cat-16 {
    				background-position: center -576px;
    			}
    			&.cat-187 {
    				background-position: center -640px;
    			}
    			&.cat-186 {
    				background-position: center -704px;
    			}
    			&.cat-821 {
    				background-position: center -768px;
    			}
    		}
    		strong {
    			width: 100%;
    			float: left;
    			color: #0076be;
    			font-size: .9375rem;
    		}

    		ul {
    			width: 100%;
    			float: left;
    			margin: .5rem 0 0 0;

    			li {
    				display: inline-block;
    				font-size: .6875rem;
    				a {
    					color: #777;
    					&:hover {
    						text-decoration: underline;
    					}
    				}
    			}
    		}
    	}
    }
}