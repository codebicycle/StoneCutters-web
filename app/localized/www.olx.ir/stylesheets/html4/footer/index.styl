.footer_footer_view {

	#footer {

		padding: 10px;

	}

	ul {

		font-size: 12px;
		text-align: center;

		li {

			display: inline;
			padding: 0 6px;
			border-left: thin solid #B5B5B5;

			&.first {

				border: none;

			}

			a {

				color: #1C7BD4;

			}

		}

	}

	p {

		padding: 8px 0 0 0;
		font-size: 10px;
		color: #333;
		text-align: center;

	}

}
