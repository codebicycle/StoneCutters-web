#footer ul {
	font-size: 12px;
	text-align: center;
}
#footer ul li {
	display: inline;
	padding: 0 6px;
	border-left: thin solid #B5B5B5;
}
#footer ul li.first {
	border: none;
}
#footer ul li a {
	color: #1C7BD4;
}
#footer p {
	padding: 8px 0 0 0;
	font-size: 10px;
	color: #333333;
	text-align: center;
}